class Solution {
public:
    string addStrings(string num1, string num2) {
        int n1 = 0, n2 = 0;
        for(int i = 0; i < num1.length(); i++){
            n1 = n1 * 10 + (int(num1[i]) - 48); 
        }
        for(int i = 0; i < num2.length(); i++){
            n2 = n2 * 10 + (int(num2[i]) - 48); 
        }
        int res = n1 + n2;
        return to_string(res);
    }
};
